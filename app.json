{
  "expo": {
    "name": "ColourSchemes",
    "slug": "ColourSchemes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tinomuzambi.ColourSchemes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tinomuzambi.ColourSchemes"
    },
    "web": {
      "favicon": "./assets/favicon.svg"
    },
    "description": "A React Native colour scheme picker.",
    "githubUrl": "https://github.com/TinoMuzambi/RNColourScheme"
  }
}
